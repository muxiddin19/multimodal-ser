      }
    },
    "qualitative_feedback": {
      "preferred_personalized": 12,
      "preferred_fixed": 2,
      "no_preference": 1
    }
  },

  "rule_based_specification": {
    "rules": [
      {
        "name": "age_font_size",
        "description": "Older adults (51+) prefer larger fonts",
