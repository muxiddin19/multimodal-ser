  "mixed_effects_analysis": {
    "user_variance": 0.182,
    "residual_variance": 0.612,
    "icc": 0.229,
    "interpretation": "22.9% of preference variance explained by individual differences",
    "fixed_effects": {
      "font_size_diff": {"coef": 0.412, "p": 0.001},
      "color_intensity_diff": {"coef": 0.234, "p": 0.012},
      "animation_speed_diff": {"coef": 0.189, "p": 0.034},
      "user_font_pref": {"coef": 0.523, "p": 0.0001},
      "user_color_pref": {"coef": 0.398, "p": 0.0005}
    },
    "model_fit": {
      "aic": 1823.4,
      "bic": 1892.1,
      "log_likelihood": -901.7
    }
  },

