{
  "study_metadata": {
    "title": "Expanded Preference Learning Study for ACL 2026",
    "n_real_users": 50,
    "comparisons_per_user": 30,
    "total_comparisons": 1500,
    "train_comparisons_per_user": 12,
    "test_comparisons_per_user": 18,
    "study_dates": "2025-12-15 to 2026-01-03",
    "irb_approval": "exempt",
    "note": "All participants are real users (no synthetic data)"
  },

  "demographics": {
    "age_groups": {
      "18-25": 12,
      "26-35": 15,
      "36-50": 10,
      "51-65": 8,
      "65+": 5
