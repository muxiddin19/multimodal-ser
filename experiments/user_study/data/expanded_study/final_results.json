    "professions": {
      "student": 15,
      "professional": 12,
      "educator": 6,
      "healthcare": 4,
      "tech": 8,
      "creative": 3,
      "retired": 2
    }
  },

  "preference_prediction": {
    "random": {
      "accuracy": 0.501,
      "std": 0.022,
      "n": 900
    },
    "rule_based": {
      "accuracy": 0.438,
      "std": 0.031,
      "n": 900,
      "note": "Below chance - demographic heuristics often wrong"
    },
    "bradley_terry": {
      "accuracy": 0.528,
      "std": 0.025,
      "n": 900,
      "note": "Hierarchical BT with user random effects"
    },
    "collaborative_filtering": {
      "accuracy": 0.535,
      "std": 0.024,
      "n": 900,
      "note": "Matrix factorization with cold-start handling"
    },
    "contextual_logistic": {
      "accuracy": 0.521,
      "std": 0.026,
      "n": 900,
      "note": "Emotion context only, no personalization"
    },
    "learned_personalized": {
      "accuracy": 0.612,
      "std": 0.028,
      "n": 900,
      "note": "Our approach: within-user learning from 12 comparisons"
    }
  },

  "by_emotion_breakdown": {
